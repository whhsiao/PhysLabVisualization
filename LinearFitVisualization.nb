Cell[CellGroupData[{
Cell["The Main Function", "Section",
 CellChangeTimes->{{3.784304127288747*^9, 
  3.784304130771368*^9}},ExpressionUUID->"377c8055-fb75-4718-a329-\
4343f641c149"],

Cell["\<\
In the following I construct a module LinearFitCom that helps us to visualize \
the linear fit of data pairs (x,y) as well as the (1-\[Alpha]) confidence \
interval bound based on the t statistics. In addition to the best fit line \
from simple least square fit I also plot two dashed lines, which provide a \
area enclosing estimators at the confidence level (1-\[Alpha]). 
The first two arguments of the function take the independent variable and the \
dependent variable in the form of lists/arrays respectively. The third \
argument is the type I error threshold value \[Alpha]. The last two arguments \
take the label on the horizontal and vertical axes.\
\>", "Text",
 CellChangeTimes->{{3.784303967199926*^9, 3.784304077540258*^9}, {
  3.784306992451511*^9, 
  3.7843072461864853`*^9}},ExpressionUUID->"87ab6d57-3ea2-498f-b734-\
1865981d1cfa"],

Cell[BoxData[
 RowBox[{
  RowBox[{"LinearFitCom", "[", 
   RowBox[{"xdata_", ",", "ydata_", ",", "alpha_", ",", 
    RowBox[{"h_:", "\"\<x\>\""}], ",", 
    RowBox[{"v_:", "\"\<y\>\""}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "XMat", ",", "VarMat", ",", "xydata", ",", "lm", ",", "S", ",", "t"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xydata", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"#1", ",", "#2"}], "}"}], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"xdata", ",", "ydata"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"XMat", " ", "=", " ", 
      RowBox[{"MapThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "#"}], "}"}], " ", "&"}], ",", 
        RowBox[{"{", "xdata", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"VarMat", " ", "=", " ", 
      RowBox[{"Inverse", "[", 
       RowBox[{"Dot", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", "XMat", "]"}], ",", "XMat"}], "]"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lm", " ", "=", " ", 
      RowBox[{"LinearModelFit", "[", 
       RowBox[{"xydata", ",", "x", ",", "x", ",", 
        RowBox[{"ConfidenceLevel", "\[Rule]", 
         RowBox[{"1", "-", "alpha"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "lm", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"lm", "[", "\"\<ParameterConfidenceIntervalTable\>\"", "]"}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"lm", "[", "\"\<ANOVATable\>\"", "]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"lm", "[", "\"\<ParameterTStatistics\>\"", "]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"S", " ", "=", " ", 
      RowBox[{"lm", "[", "\"\<EstimatedVariance\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"t", " ", "=", " ", 
      RowBox[{"InverseSurvivalFunction", "[", 
       RowBox[{
        RowBox[{"StudentTDistribution", "[", 
         RowBox[{
          RowBox[{"Length", "[", "xdata", "]"}], "-", "2"}], "]"}], ",", 
        RowBox[{"alpha", "/", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"ListPlot", "[", 
        RowBox[{"xydata", ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"PointSize", "[", "Medium", "]"}]}], ",", 
         RowBox[{"PlotTheme", "->", "\"\<Classic\>\""}]}], "]"}], ",", 
       RowBox[{"Plot", "[", 
        RowBox[{
         RowBox[{"lm", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"Min", "[", "xdata", "]"}], ",", 
           RowBox[{"Max", "[", "xdata", "]"}]}], "}"}], ",", 
         RowBox[{"PlotTheme", "->", "\"\<Classic\>\""}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", "Blue"}]}], "]"}], ",", 
       RowBox[{"Plot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"lm", "[", "x", "]"}], "+", 
          RowBox[{"t", "*", 
           RowBox[{"Sqrt", "[", "S", "]"}], "*", 
           RowBox[{"Sqrt", "[", 
            RowBox[{"1", "+", 
             RowBox[{"Dot", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"1", ",", "x"}], "}"}], ",", 
               RowBox[{"Dot", "[", 
                RowBox[{"VarMat", ",", 
                 RowBox[{"{", 
                  RowBox[{"1", ",", "x"}], "}"}]}], "]"}]}], "]"}]}], 
            "]"}]}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"Min", "[", "xdata", "]"}], ",", 
           RowBox[{"Max", "[", "xdata", "]"}]}], "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"Red", ",", 
            RowBox[{"Thickness", "[", "0.004", "]"}], ",", 
            RowBox[{"Dashing", "[", "Tiny", "]"}]}], "}"}]}], ",", 
         RowBox[{"PlotTheme", "->", "\"\<Classic\>\""}]}], "]"}], ",", 
       RowBox[{"Plot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"lm", "[", "x", "]"}], "-", 
          RowBox[{"t", "*", 
           RowBox[{"Sqrt", "[", "S", "]"}], "*", 
           RowBox[{"Sqrt", "[", 
            RowBox[{"1", "+", 
             RowBox[{"Dot", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"1", ",", "x"}], "}"}], ",", 
               RowBox[{"Dot", "[", 
                RowBox[{"VarMat", ",", 
                 RowBox[{"{", 
                  RowBox[{"1", ",", "x"}], "}"}]}], "]"}]}], "]"}]}], 
            "]"}]}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"Min", "[", "xdata", "]"}], ",", 
           RowBox[{"Max", "[", "xdata", "]"}]}], "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"Red", ",", 
            RowBox[{"Thickness", "[", "0.004", "]"}], ",", 
            RowBox[{"Dashing", "[", "Tiny", "]"}]}], "}"}]}], ",", 
         RowBox[{"PlotTheme", "->", "\"\<Classic\>\""}]}], "]"}], ",", 
       RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
       RowBox[{"AxesLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Style", "[", 
           RowBox[{"h", ",", 
            RowBox[{"FontSize", "\[Rule]", "16"}], ",", "Italic"}], "]"}], 
          ",", 
          RowBox[{"Style", "[", 
           RowBox[{"v", ",", 
            RowBox[{"FontSize", "\[Rule]", "16"}], ",", "Italic"}], "]"}]}], 
         "}"}]}], ",", 
       RowBox[{"LabelStyle", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"FontFamily", "\[Rule]", "\"\<Times New Roman\>\""}], ",", 
          "14"}], "}"}]}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.784299357150648*^9, 3.784299496532012*^9}, {
